{
   "Logging": {
      "LogLevel": {
         "Default": "Information",
         "Microsoft": "Warning",
         "Microsoft.Hosting.Lifetime": "Information"
      }
   },
   "AllowedHosts": "*",
   
   "ReverseProxy": {
      "Routes": {
         "api-social" : {
            "ClusterId": "cluster-api-social",
            "Match": {
               "Path": "{**catch-all}"
            }
         }
      },
      "Clusters": {
         "cluster-api-social": {
            "Destinations": {
               "destination1": {
                  "Address": "http://localhost:5238"
               }
            }
         }
      }
   }
}